<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard1.4;net40;net45;net46</TargetFrameworks>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Authors>Nossila</Authors>
    <Company>Nossila</Company>
    <PackageId>Nossila.Text.FixedLength</PackageId>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
    <Version>1.0.3</Version>
    <Description>A small, short and direct library to serialize and parse fixed length content.</Description>
    <Copyright />
    <PackageTags>fixed-length fixed length text positional converter serializer deserializer</PackageTags>
    <PackageProjectUrl>https://github.com/alissan1/nossila-text-fixedlength</PackageProjectUrl>
    <RepositoryUrl>https://github.com/alissan1/nossila-text-fixedlength</RepositoryUrl>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\LICENSE">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
